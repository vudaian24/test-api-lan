{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": "dist/**" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["OPTIONS", "GET", "POST", "PUT", "PATCH", "DELETE", "HEAD"]
    }
  ]
}
